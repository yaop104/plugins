			<if test="tapApplicationOrder!=null" >tap_application_order=#{tapApplicationOrder,jdbcType=INTEGER},</if>
			<if test="tapApplicationCdate!=null" >tap_application_cdate=#{tapApplicationCdate,jdbcType=TIMESTAMP},</if>
